<nav>
    <button class=".nav-left" type="button">Sell a property</button>
    <button class=".nav-left" type="button">Buy a property</button>
    </nav>
<div> 
  <br>
<a href="/"> &#8592; Home</a>
  <br><br>
  <h1>{{title}}</h1>
  <p>Username: {{user.username}}</p> 
  <p>First Name: {{user.userFirstName}}</p> 
  <p>Last Name:{{user.userLastName}}</p> 
  <p>Email: {{user.email}}</p> 
  <p>Phone number: {{user.phoneNumber}}</p> 
  <p>Age: {{user.age}}</p> 
  <dl>
    <dt>Properties</dt> 
      <dd> 
        <ul>
          {{#each user.properties}} 
          <li>{{this}}</li>
          {{else}}
          <p class="empty">You do not have any property to sell yet</p>
          {{/each}}
        </ul> 
      </dd> 
    <dt>Favorites</dt> 
      <dd> 
        <ul>
          {{#each user.favorites}} 
          <li>{{this}}</li>
          {{else}}
          <p class="empty">You do not have any favorites yet</p>
          {{/each}}
        </ul> 
      </dd>
<br>
<a href="/users/logout" class="logout"  type="button">Log Out</a>
</div>

